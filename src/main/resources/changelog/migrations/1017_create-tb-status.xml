    <changeSet id="1017.0" author="vborovok">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="status"/>
            </not>
        </preConditions>
        <createTable tableName="status">
            <column name="code" type="varchar(10)">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>
            <column name="state" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="expired_days_num" type="bigint" defaultValue="0">
                <constraints nullable="true"/>
            </column>
            <column name="expired_status" type="varchar(10)" defaultValue="0">
                <constraints nullable="true"/>
            </column>
            <column name="life_span" type="bigint" defaultValue="0">
                <constraints nullable="true"/>
            </column>
            <column name="life_span_status" type="varchar(10)" defaultValue="0">
                <constraints nullable="true"/>
            </column>
            <column name="route_type" type="varchar(50)">
                <constraints nullable="false" foreignKeyName="fk_route_type" references="route_types(code)"/>
            </column>
            <column name="description" type="varchar(100)">
                <constraints nullable="true"/>
            </column>
            <column name="disabled_on" type="timestamp">
                <constraints nullable="true"/>
            </column>
        </createTable>
        <rollback>
            <dropTable tableName="status"/>
        </rollback>
    </changeSet>
